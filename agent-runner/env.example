# Plane API (base URL and workspace slug are in config.json)
PLANE_API_KEY=your-plane-api-key

# Anthropic (for Claude Code SDK)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Telegram (same bot token as telegram-bot)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-telegram-chat-id

# GitHub (for git push in worktrees)
GITHUB_PAT=your-github-personal-access-token

# GitHub Webhook (optional - for auto-updating tasks when PRs are merged)
# Generate a random secret and configure it in your GitHub repository webhook settings
GITHUB_WEBHOOK_SECRET=your-webhook-secret
