{
  "plane": {
    "baseUrl": "http://localhost:8000/api/v1",
    "workspaceSlug": "projects"
  },
  "projects": {
    "AGENTHQ": {
      "repoPath": "/repos/agent-hq",
      "repoUrl": "https://github.com/dobosmarton/agent-hq",
      "defaultBranch": "main",
      "planeIdentifier": "AGENTHQ"
    },
    "AOE2AGENT": {
      "repoPath": "/repos/aoe2-agent",
      "repoUrl": "https://github.com/dobosmarton/aoe2-agent",
      "defaultBranch": "main",
      "planeIdentifier": "AOE2AGENT"
    },
    "VERDANDI": {
      "repoPath": "/repos/verdandi",
      "repoUrl": "https://github.com/dobosmarton/verdandi",
      "defaultBranch": "main",
      "planeIdentifier": "VERDANDI"
    },
    "STYLESWIPE": {
      "repoPath": "/repos/style-swipe-app",
      "repoUrl": "https://github.com/Style-swipe/style-swipe-app",
      "defaultBranch": "main",
      "planeIdentifier": "STYLESWIPE"
    }
  },
  "agent": {
    "maxConcurrent": 4,
    "maxBudgetPerTask": 5.0,
    "maxDailyBudget": 20.0,
    "maxTurns": 200,
    "pollIntervalMs": 30000,
    "spawnDelayMs": 15000,
    "maxRetries": 2,
    "retryBaseDelayMs": 60000,
    "labelName": "agent"
  },
  "review": {
    "enabled": false,
    "triggerOnOpened": true,
    "triggerOnSynchronize": true,
    "severityThreshold": "major",
    "maxDiffSizeKb": 100,
    "claudeModel": "claude-3-5-sonnet-20241022"
  }
}
